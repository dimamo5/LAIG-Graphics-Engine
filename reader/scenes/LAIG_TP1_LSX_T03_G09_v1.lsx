        <MATERIAL id="defaultMaterial" />       
        <TEXTURE id="null" />                 
        <DESCENDANTS>
            <DESCENDANT id="chao" />
            <DESCENDANT id="paredes" />
            <DESCENDANT id="mesa" /> 
            <DESCENDANT id="candeeiro" /> 
            <DESCENDANT id="lenha" />
            <DESCENDANT id="fardos" />
           <!-- <DESCENDANT id="armario" />-->


        </DESCENDANTS>
    </NODE>
    
    <NODE id="chao">      
