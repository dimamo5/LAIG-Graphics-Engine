<SCENE>

    <INITIALS>
        <frustum near="0.1" far="200" />
        <translation x="0.0" y="0.0" z="0.0" />
        <rotation axis="y" angle="0.0" />
        <rotation axis="x" angle="0.0" />
        <rotation axis="z" angle="0.0" />
        <scale sx="1.0" sy="1.0" sz="1.0" />
        <reference length="5.0" />
    </INITIALS>


    <ILLUMINATION>
        <ambient r="0.83" g="0.92" b="1.0" a="1.0" />
        <background r="0.0" g="0.0" b="0.0" a="1.0" />
    </ILLUMINATION>

<LIGHTS>
    <LIGHT id="LightA">                                    
        <enable value ="true" />                         
        <position x="12.0" y="12.0" z="12.0" w="1.0" />       
        <ambient r="0.4" g="0.4" b="0.54" a="1.0" />        
        <diffuse r="0.4" g="0.4" b="0.54" a="1.0" />        
        <specular r="0.7" g="0.7" b="0.7" a="1.0" />       
    </LIGHT>

   <LIGHT id="LightB">                                    
        <enable value ="true" />                         
        <position x="4.0" y="12.0" z="5.5" w="0.0" />       
        <ambient r="0.3" g="0.3" b="0.3" a="1.0" />        
        <diffuse r="0.3" g="0.3" b="0.3" a="1.0" />        
        <specular r="0.9" g="0.9" b="0.9" a="0.9" />       
    </LIGHT>

    <LIGHT id="LightC">                                    
        <enable value ="true" />                         
        <position x="10.0" y="6.65" z="8.0" w="0.0" />       
        <ambient r="0.5" g="0.5" b="0.5" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />        
        <specular r="0.9" g="0.9" b="0.9" a="1.0" />  
     </LIGHT>      
</LIGHTS>


<TEXTURES>

    <!-- Wood -->
    <TEXTURE id="wood">
        <file path="textures/wood.jpg" />                           
        <amplif_factor s="1.0" t="1.0" />
    </TEXTURE>

    <!-- Walls -->
    <TEXTURE id="walls">
        <file path="textures/walls.jpg" />
        <amplif_factor s="3.0" t="3.0" />
    </TEXTURE>

    <!-- Auxiliary Wall -->
    <TEXTURE id="auxiliaryWall">
        <file path="textures/walls.jpg" />
        <amplif_factor s="1.0" t="3.0" />
    </TEXTURE>

    <!-- Wallpapper -->
    <TEXTURE id="wallpaper">
        <file path="textures/wallpaper.jpg" />                           
        <amplif_factor s="1.0" t="1.0" />
    </TEXTURE>

     <!-- Floor -->
    <TEXTURE id="floor">
        <file path="textures/floor.jpg" />
        <amplif_factor s="4.0" t="4.0" />
    </TEXTURE> 

    <!-- Timber -->
    <TEXTURE id="timber">
        <file path="textures/timber.jpg" />                           
        <amplif_factor s="1.0" t="1.0" />
    </TEXTURE>

    <!-- Palha -->
     <TEXTURE id="palha">
        <file path="textures/palha.jpg" />                           
        <amplif_factor s="1.0" t="1.0" />
    </TEXTURE>

</TEXTURES>


<MATERIALS>
     
     <MATERIAL id="defaultMaterial">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />       
        <ambient r="0.0" g="0.0" b="0.0" a="1.0" />         
        <emission r="0.0" g="0.0" b="0.0" a="1.0" />        
    </MATERIAL>

     <MATERIAL id="CorBolaCandeeiro">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.5" g="0.5" b="0.5" a="1.0" />       
        <ambient r="1.0" g="1.0" b="0.0" a="1.0" />         
        <emission r="0.0" g="0.0" b="0.0" a="1.0" />        
    </MATERIAL>

    <MATERIAL id="CorFioCandeeiro">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />       
        <ambient r="1.0" g="0.0" b="0.0" a="1.0" />         
        <emission r="0.0" g="0.0" b="0.0" a="1.0" />        
    </MATERIAL>

    <MATERIAL id="view">                                  
        <shininess value="1" />
        <specular r="0.0" g="0.0" b="0.0" a="1.0" />        
        <diffuse r="0.0" g="0.0" b="0.0" a="1.0" />       
        <ambient r="1.0" g="0.0" b="0.0" a="1.0" />         
        <emission r="1.0" g="1.0" b="1.0" a="1.0" />        
    </MATERIAL>

</MATERIALS>


<!-- The leaves -->
<LEAVES>
    <LEAF id="rectangulo" type="rectangle" args="0.0 0.0 1.0 1.0" />
    <LEAF id="cilindro" type="cylinder" args="1.0 1.0 1.0 16 16" /> 
    <LEAF id="esfera" type="sphere" args="1 16 16" />
    <LEAF id="triangulo" type="triangle" args="2 0 12 12 0 2 0 0 0" />
</LEAVES>


<NODES>
    <ROOT id="root" />     
    
