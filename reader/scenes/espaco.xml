        <diffuse r="0.5" g="0.5" b="0.5" a="1"  />         <!-- diffuse reflection -->
        <ambient r="0.5" g="0.5" b="0.5" a="1"   />         <!-- ambient reflection -->
        <emission r="0.9" g="0.9" b="0.0" a="1"  />        <!-- emission component -->
