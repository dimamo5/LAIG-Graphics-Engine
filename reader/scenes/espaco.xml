    <MATERIAL id="base">                                  
        <shininess value="15" />
        <specular r="0.1" g="0.1" b="0.1" a="1" />        <!-- specular reflection -->
        <diffuse r="0.1" g="0.1" b="0.1" a="1" />         <!-- diffuse reflection -->
        <ambient r="0.1" g="0.1" b="0.1" a="1" />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1" />        <!-- emission component -->
    </MATERIAL>

    <MATERIAL id="mat_asas">                                  
        <shininess value="50" />
        <specular r="0.92" g="0.1" b="0.14" a="1" />        <!-- specular reflection -->
        <diffuse r="0" g="0" b="0" a="1"  />         <!-- diffuse reflection -->
        <ambient r="0" g="0" b="0" a="1"  />         <!-- ambient reflection -->
        <emission r="0" g="0" b="0" a="1"  />        <!-- emission component -->
    </MATERIAL>

