    <MATERIAL id="mat_terra">
        <shininess value="100" />
        <specular r="0" g="0" b="0" a="1" />        <!-- specular reflection -->
        <diffuse r="0.2" g="0.2" b="0.2" a="1"  />         <!-- diffuse reflection -->
        <ambient r="0.2" g="0.2" b="0.2" a="1"  />         <!-- ambient reflection -->
