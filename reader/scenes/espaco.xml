        <diffuse r="0.8" g="0.8" b="0.8" a="1"  />         <!-- diffuse reflection -->
        <ambient r="0.8" g="0.8" b="0.8" a="1"   />         <!-- ambient reflection -->
        <emission r="0.9" g="0.9" b="0.0" a="1"  />        <!-- emission component -->
    </MATERIAL>

    <MATERIAL id="cor_cil">                                  
        <shininess value="15" />
        <specular r="0.9" g="0.9" b="0.9" a="1" />        <!-- specular reflection -->
        <diffuse r="0.1" g="0.1" b="0.1" a="1"  />         <!-- diffuse reflection -->
        <ambient r="0.1" g="0.1" b="0.1" a="1"  />         <!-- ambient reflection -->
