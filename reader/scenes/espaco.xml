<SCENE>

<INITIALS>
    <frustum near="0.1" far="500"/>                      <!-- frustum planes-->
    <translation x="-20" y="-30" z="-40" />                 <!-- initial translate -->
    <rotation axis="z" angle="0" />                  <!-- initial rotation 3 -->
    <rotation axis="x" angle="0" />                  <!-- initial rotation 2 -->
    <rotation axis="y" angle="-45" />                  <!-- initial rotation 1 -->
    <scale sx="1" sy="1" sz="1" />                  <!-- initial scaling -->
    <reference length="0" />                          <!-- axis length; "0" means no axis-->
</INITIALS>


<ILLUMINATION>
    <ambient r="0.6" g="0.6" b="0.6" a="1.0" />            <!-- global ambient -->
    <background r="0" g="0" b="0" a="1" />         <!-- background color -->
</ILLUMINATION>


<LIGHTS>
    <LIGHT id="light1">                                    <!-- light identifier -->
        <enable value="1" />                         <!-- enable/disable -->
        <position x="0" y="2.2" z="0" w="1" />       <!-- light position -->
        <ambient r="0" g="0" b="0" a="1.0" />        	 
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" /> 
    </LIGHT>
    <LIGHT id="light2">                                    <!-- light identifier -->
        <enable value="1" />                         <!-- enable/disable -->
        <position x="8" y="5" z="8" w="1" />       <!-- light position -->
        <ambient r="0" g="0" b="0" a="1.0" />        	 
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" /> 
    </LIGHT>
    <LIGHT id="light3">                                    <!-- light identifier -->
        <enable value="1" />                         <!-- enable/disable -->
        <position x="30" y="20" z="30" w="1" />       <!-- light position -->
        <ambient r="0" g="0" b="0" a="1.0" />        	 
        <diffuse r="1.0" g="1.0" b="1.0" a="1.0" />        
        <specular r="1.0" g="1.0" b="1.0" a="1.0" /> 
    </LIGHT>
    

    <!-- NOTE: this block "LIGHT" must be repeated as necessary with different "id" -->
    
</LIGHTS>


<TEXTURES>
    <TEXTURE id="text_papel">
        <file path="./resources/paper.JPG"/>                             <!-- path to file -->
        <amplif_factor s="0.5" t="0.5" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="text_solo_lunar">
        <file path="./resources/solo_lunar.JPG"/>                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="text_lateral">
        <file path="./resources/lateral2.png"/>                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="text_estrela">
        <file path="./resources/estrelas.JPG"/>                             <!-- path to file -->
        <amplif_factor s="10" t="10" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="text_terra">
        <file path="./resources/terra.JPG"/>                             <!-- path to file -->
        <amplif_factor s="6.1415" t="6.1415" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="text_telhado">
        <file path="./resources/telhado.JPG"/>                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>

    <TEXTURE id="text_parede">
        <file path="./resources/parede.JPG"/>                             <!-- path to file -->
        <amplif_factor s="1" t="1" />                <!-- x/s, y/t -->
    </TEXTURE>
    <TEXTURE id="text_porta">
        <file path="./resources/porta.JPG"/>                             <!-- path to file -->
        <amplif_factor s="2" t="2" />                <!-- x/s, y/t -->
    </TEXTURE>

    <!-- NOTE: this block "TEXTURE" must be repeated as necessary with different "id" -->
    
